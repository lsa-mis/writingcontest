#WritingContest Site Security
#Web Space Restriction Description

# MAINTENANCE-PAGE REDIRECT
<IfModule mod_rewrite.c>
 RewriteEngine on
# RewriteCond %{REMOTE_ADDR} !^35\.7\.40\.33
 RewriteCond %{REQUEST_URI} !/closed-index.php$ [NC]
 RewriteCond %{REQUEST_URI} !\.(jpe?g?|png|gif) [NC]
 RewriteRule .* /WritingContest/closed-index.php [R=302,L]
</IfModule>

#CosignProtected On
#AuthType Cosign
#CosignRequireFactor   UMICH.EDU
#SSLRequireSSL
#AuthLDAPURL ldap://ldap.umich.edu
#Require valid-user
